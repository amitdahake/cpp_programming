#include <iostream>
using namespace std;

int main()
{
    //creating an array with 5 random values

    int arr[5] = {77, 22, 17, 100, 256};
    int a;

    //printing the values in array

    for (int i =0; i<5 ; i++){
        cout<<arr[i]<<" , ";
    }
    cout<<endl;
    cout<<"acsending value by sorting:"<<endl;

    //printing the vale in array by sorting in ascending order in vertical.

    for(int i = 0; i<5; i++){
        for(int j = 0; j<5; j++){
            if (arr[i]>arr[j]){
                continue;
        }else{
               a = arr[i];
               arr[i]=arr[j];
               arr[j]=a;
        }
    }
    }
     for (int i =0; i<5 ; i++){
        cout<<arr[i]<<"\n";}


return 0;
}

